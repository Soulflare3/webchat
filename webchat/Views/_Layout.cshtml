<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8">

    <link rel="shortcut icon" href="@Url.Content("~/Static/favicon.ico")" />
    
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"
        type="text/javascript"></script>

    <script type="text/javascript">
        if (typeof jQuery == 'undefined') {
            console.log("ERROR, jQuery, not loaded from CDN!");
            document.write(decodeURI("%3Cscript src='@Url.Content("~/Static/Js/lib/jquery.min.js")' type='text/javascript'%3E%3C/script%3E"));
        }
    </script>

    <link type="text/css" href="@Url.Content("~/Static/Css/jquery-ui-1.8.22.custom.css")" rel="stylesheet" />
    <link type="text/css" href="@Url.Content("~/Static/Css/bootstrap.min.css")" rel="stylesheet" />
    <link type="text/css" href="@Url.Content("~/Static/Css/style.css")" rel="stylesheet" />

    <title>Webchat</title>
</head>
<body>
    @RenderBody()

    <p class="footer">
        @Html.Raw(string.Format(Resources.Strings.FooterText, "<a href=\"http://glyphicons.com\">Glyphicons Free</a>",
        "<a href=\"http://creativecommons.org/licenses/by/3.0/\">CC BY 3.0</a>"))
    </p>
</body>
</html>
